{
	"name": "Valhalla Theme",
	"type": "dark",
	"colors": {
		
		/* Window*/
		"window.activeBorder": "#01A894",
		"foreground": "#dde1d8",
		"focusBorder": "#01A894",
		"selection.background": "#4A4B5E",

		/* Text*/
		"textBlockQuote.background": "#4A4B5E",
		"textBlockQuote.border": "#49bbf7",
		"textCodeBlock.background": "#4A4B5E",
		"textLink.foreground": "#49bbf7",
		"textLink.activeForeground": "#01A894",

		/* Toolbar*/
		"toolbar.hoverBackground": "#4A4B5E",
		"toolbar.hoverOutline": "#4A4B5E",
		"toolbar.activeBackground": "#4A4B5E",

		/* Button control */
		"button.background": "#2B2E46",
		"button.foreground": "#dde1d8",
		"button.border": "#01A894",
		"button.hoverBackground": "#4A4B5E",

		/* Dropdown Control*/
		"dropdown.background": "#2B2E46",
		"dropdown.foreground": "#dde1d8",
		"dropdown.listBackground": "#2B2E46",
		
		/* Input Control*/
		"input.background": "#4A4B5E",
		"input.foreground": "#dde1d8",
		"input.border": "#01A894",
		"input.placeholderForeground": "#dde1d8",
		"inputOption.activeBackground": "#2B2E46",
		"inputOption.activeBorder": "#01A894",
		"quickInput.background": "#222538",
		
		/* Scrollbar Control*/
		"scrollbar.shadow": "#2B2E46",
		"scrollbarSlider.background": "#01A894",
		
		/* Badge*/
		"badge.background": "#2b2e46",
		"badge.foreground": "#dde1d8",

		/* Activity Bar*/
		"activityBar.background": "#222538",
		"activityBar.foreground": "#dde1d8",
		"activityBarBadge.background": "#01A894",
		"activityBarBadge.foreground": "#dde1d8",
		"statusBar.foreground": "#dde1d8",
		"activityBar.activeBorder": "#01A894",
		"titleBar.activeBackground": "#272b41",

		/* Side bar*/
		"sideBar.background": "#272b41",
		"sideBar.foreground": "#dde1d8",
		"sideBarSectionHeader.background" : "#272b41",
		"sideBarSectionHeader.foreground": "#dde1d8",
		"sideBarTitle.foreground": "#dde1d8",

		/* Status bar*/
		"statusBar.background": "#01A894",

		/* Editor*/
		"editor.background": "#2B2E46",
		"editor.foreground": "#dde1d8",
		"editorInfo.foreground": "#dde1d8",
		"editorGroupHeader.tabsBackground": "#272b41",
		"editorInlayHint.background": "#4A4B5E",
		"editorInlayHint.foreground": "#dde1d8",
		"editorWidget.background": "#222538",
		"editorWidget.border": "#01A894",
		"editor.lineHighlightBackground": "#4A4B5E",
		"editor.findMatchBackground": "#49baf77e",
		"editor.findMatchHighlightBackground": "#f1ff8985",
		"editor.selectionBackground": "#01a89583",

		/* Tabs*/
		"tab.activeForeground": "#dde1d8",
		"tab.activeBackground": "#2B2E46",
		"tab.inactiveBackground": "#4A4B5E",
		"tab.inactiveForeground": "#dde1d8",
		"tab.border": "#01A894",
		
		/* List*/		
		"list.activeSelectionBackground": "#4A4B5E",
		"list.activeSelectionForeground": "#dde1d8",
		"list.hoverBackground": "#4A4B5E",
		"list.hoverForeground": "#dde1d8",
		"list.inactiveSelectionBackground": "#222538",
		"list.inactiveSelectionForeground": "#dde1d8",

		/* Terminal */
		"panel.border": "#01A894",
		"progressBar.background": "#01A894"
		
	},
	
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#A288E3"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"fontStyle": "italic bold",
				"foreground": "#b752e6"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#BBD5ED"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#f2ff89"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#c283ec"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.control",
				"constant.other.color",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#49bbf7"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#49bbf7"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#49bbf7"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#b752e6"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#b752e6"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#66f9ff"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"foreground": "#29eed7"
			}
		},
		{
			"name": "Class, Support",
			"scope": [
				"entity.name",
				"support.type",
				"support.class",
				"support.other.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#f2ff89"
			}
		},
		{
			"name": "Entity Types",
			"scope": [
				"support.type"
			],
			"settings": {
				"foreground": "#BBD5ED"
			}
		},
		{
			"name": "CSS Class and Support",
			"scope": [
				"source.css support.type.property-name",
				"source.sass support.type.property-name",
				"source.scss support.type.property-name",
				"source.less support.type.property-name",
				"source.stylus support.type.property-name",
				"source.postcss support.type.property-name"
			],
			"settings": {
				"foreground": "#49bbf7"
			}
		},
		{
			"name": "Sub-methods",
			"scope": [
				"entity.name.module.js",
				"variable.import.parameter.js",
				"variable.other.class.js"
			],
			"settings": {
				"foreground": "#f2ff89"
			}
		},
		{
			"name": "Language methods",
			"scope": [
				"variable.language"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#f2ff89"
			}
		},
		{
			"name": "entity.name.method.js",
			"scope": [
				"entity.name.method.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#82AAFF"
			}
		},
		{
			"name": "meta.method.js",
			"scope": [
				"meta.class-method.js entity.name.function.js",
				"variable.function.constructor"
			],
			"settings": {
				"foreground": "#82AAFF"
			}
		},
		{
			"name": "Attributes",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#ad68ce"
			}
		},
		{
			"name": "HTML Attributes",
			"scope": [
				"text.html.basic entity.other.attribute-name.html",
